{
  "library": {
    "rrd_memory": {
      "class": "com.qwazr.tools.Rrd4jTool",
      "path": "memory.rrd",
      "backendFactory": "MEMORY",
      "step": 60,
      "datasources": [
        {
          "dsName": "freeMemory",
          "dsType": "COUNTER",
          "heartbeat": 120,
          "minValue": 0
        },
        {
          "dsName": "maxMemory",
          "dsType": "COUNTER",
          "heartbeat": 120,
          "minValue": 0
        },
        {
          "dsName": "totalMemory",
          "dsType": "COUNTER",
          "heartbeat": 120,
          "minValue": 0
        }
      ],
      "archives": [
        {
          "consolFun": "AVERAGE",
          "xff": 0.5,
          "steps": 1,
          "rows": 600
        },
        {
          "consolFun": "TOTAL",
          "xff": 0.5,
          "steps": 1,
          "rows": 600
        },
        {
          "consolFun": "MAX",
          "xff": 0.5,
          "steps": 1,
          "rows": 600
        }
      ]
    },
    "xml": {
      "class": "com.qwazr.tools.XMLTool"
    },
    "csv": {
      "class": "com.qwazr.tools.CSVTool"
    },
    "freemarker_classloader": {
      "class": "com.qwazr.tools.FreeMarkerTool",
      "use_classloader": true,
      "output_encoding": "UTF-8",
      "default_encoding": "UTF-8",
      "default_content_type": "TEXT/HTML"
    },
    "freemarker_files": {
      "class": "com.qwazr.tools.FreeMarkerTool",
      "output_encoding": "UTF-8",
      "default_encoding": "UTF-8",
      "default_content_type": "TEXT/HTML"
    },
    "thymeleaf_classloader": {
      "class": "com.qwazr.tools.ThymeleafTool",
      "use_classloader": true,
      "cache_enabled": true,
      "cache_ttl_ms": 60000,
      "template_mode": "XHTML",
      "suffix": ".html"
    },
    "thymeleaf_files": {
      "class": "com.qwazr.tools.ThymeleafTool",
      "cache_enabled": false,
      "character_encoding": "UTF-8",
      "suffix": ".html",
      "template_mode": "XHTML"
    },
    "markdown": {
      "class": "com.qwazr.tools.MarkdownTool",
      "extensions": [
        "fenced_code_blocks",
        "autolinks"
      ],
      "max_parsing_time": 10000
    },
    "markdown_bootstrap": {
      "class": "com.qwazr.tools.MarkdownTool",
      "extensions": [
        "tables"
      ],
      "html_serializer_class": "com.qwazr.tools.MarkdownTool$BootstrapHtmlSerializer"
    },
    "archiver": {
      "class": "com.qwazr.tools.ArchiverTool",
      "codec": "deflate"
    },
    "gzip_archiver": {
      "class": "com.qwazr.tools.ArchiverTool",
      "codec": "gzip"
    }
  }
}